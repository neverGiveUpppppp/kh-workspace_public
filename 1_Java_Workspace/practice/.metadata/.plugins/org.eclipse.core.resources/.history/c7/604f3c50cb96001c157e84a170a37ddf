


public class practice_everyday4_1 {
	
	private practice_everyday4_0 s = new practice_everyday4_0();
	
	public String saveData(String kind, String name, String flavor, int numOf, int price) {
		s.setKind(kind);
		s.setName(name);
		s.setFlavor(flavor);
		s.setNumOf(numOf);
		s.setPrice(price);
		return "저장 완료되었습니다";
	}
	
		
}