

// 1.메소드
// 2.클래스 소속 메소드
// 3.인스턴스 소속 메소드


// 2.클래스 소속 메소드
class first {
	public static void last1() {				 // last1() 차이
		System.out.println("메소드");				 // static O
		System.out.println("메소드");
	}
	public static void last2(String delimiter) { // last2() 차이
		System.out.println(delimiter);			 // 구분자 delimiter
		System.out.println("메소드");				 // static O
		System.out.println("메소드");
	}
}
public class practice_everyday {
	public static void main(String[] args) {
		
		first.last1();				// 클래스 소속 메소드						
		first.last2("====");		// 위에 class 소속 method에서 static 필요
		
		
	}
}
