

// 메소드
// 클래스 소속 메소드
// 인스턴스 소속 메소드


public class practice_everyday {
	public static void last1() {
		System.out.println("메소드");
		System.out.println("메소드");
	}
	public static void last2(String delimiter) {
		System.out.println(delimiter);
		System.out.println("메소드");
		System.out.println("메소드");
	}
	public static void main(String[] args) {
		
		last1("----");
		last2("====");
		
		
	}
}
