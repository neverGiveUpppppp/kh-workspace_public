
// run
public class practice_everyday04 {
	public static void main(String[] args) {
	
		public void call01() {
			System.out.println("call_01 on");
			call_02();
			System.out.println("call_01 off");
		}
		public void call_02() {
			System.out.println("call_02 on");
			call_02();
			System.out.println("call_02 off");
		}
		public void call_03() {
			System.out.println("call_03 on");
			System.out.println("call_03 off");
		}
		
		
	}	
}