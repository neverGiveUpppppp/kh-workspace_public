		public void call01() {
			System.out.println("call01 on");
			call_02();
			System.out.println("call01 off");
		}
		public void call_02() {
			System.out.println("call01 on");
			call_02();
			System.out.println("call01 off");
		}
		public void call_03() {
			System.out.println("call01 on");
			call_03();
			System.out.println("call01 off");
		}
		
		
	}	
