import java.io.IOException;

public class practice_everyday4_0 {
	
	public void call_1() {
		System.out.println("call_1 on");
		call_2();
		System.out.println("call_1 off");
	}
	public void call_2() {
		System.out.println("call_2 on");
		call_3();
		System.out.println("call_2 off");
	}
	public void call_3() {
		try {
			System.out.println("call_3 on");
			throw new IOException();
			System.out.println("call_3 off");
		} catch(IOException e) {
			e.printStackTrace();
		}
	}
	
		
}