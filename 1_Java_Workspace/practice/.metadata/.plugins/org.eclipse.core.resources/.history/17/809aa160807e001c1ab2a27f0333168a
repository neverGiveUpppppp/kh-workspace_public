

class pprint {
	public String delimiter;
	public static void robot01(String delimiter) {
		System.out.println(delimiter);
		System.out.println("Kim's");
	}
	public void robot02() {
		System.out.println(this.delimiter);
		System.out.println("Lee's");

	}
}

public class practice_everyday04 {
	
	public static void main(String[] args) {
		
		pprint.robot01("@@@@");
		
		pprint iron02 = new pprint();
		iron02.delimiter = "^^^^";
		iron02.robot02();
		
		
	}
}