package com.kh.practice.snack.run;

import java.awt.Menu;
import java.util.Scanner;

public class Run {

	public static void main(String[] args) {

		// menu() 호출
		Scanner sc = new Scanner(System.in);
		menu m = new menu();
		
		System.out.println("스낵류를 입력하세요.");
		
		m.setKind("빵");
		sc.nextLine();
		m.setName("케이크");
		sc.nextLine();
		m.setFlavor("블루베리");
		sc.nextLine();
		m.setNumOf(1);
		sc.nextInt();
		m.setPrice(15000);
		sc.nextInt();
		System.out.println("저장 완료되었습니다");
		System.out.println("저장한 정보를 확인하시겠습니까?(y/n) : y");
		

		m.confirmData();
		
		
	}

}
