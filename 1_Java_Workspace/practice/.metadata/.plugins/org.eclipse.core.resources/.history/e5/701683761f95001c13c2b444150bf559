package com.kh.example.chap04_Math.controller;

public class MathController {

	public void method() {
		double num = 4.949;
		
		// 클래스 안에는 필드도 많다
		// ex) Math.PI
		double pi = Math.PI; 	// 필드: Math.PI
		System.out.println("파이 : "+pi);
				
		double abs = Math.abs(num);
		System.out.println("절대값 : "+abs); // 절대값이란? 
		
		
		
	}
	
}
